<?xml version="1.0" encoding="UTF-8"?>


<!-- beans tag (spring_schema) -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<!-- bean tag -->
	<bean id="e1" class="Employee">
				<property name="name" value="likhitha" />
				<property name="age" value="27" />
				<property name="add" value="bangalore" />

			</bean>

			<bean id="e2" class="Employee">
				<property name="name" value="hima" />
				<property name="age" value="28" />
				<property name="add" value="Detroit" />


			</bean>

			<bean id="e3" class="Employee">
				<property name="name" value="tanny" />
				<property name="age" value="25" />
				<property name="add" value="Detroit" />


			</bean>
	<bean id="r" class="Report">
		<property name="map">
			<map>
				<entry key="1" value-ref="e1"></entry>
				<entry key="2" value-ref="e2"></entry>
				<entry key="3" value-ref="e3"></entry>
			</map>
		</property>
	</bean>

</beans>

